/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{
  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */
  

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_DMA_Init();
  MX_USART1_UART_Init();
  MX_I2C2_Init();
  MX_TIM2_Init();
  MX_TIM3_Init();
  MX_TIM4_Init();
  /* USER CODE BEGIN 2 */
  OLED_Init();
  OLED_CLS();
  HAL_TIM_PWM_Start(&htim3,TIM_CHANNEL_1);
  HAL_TIM_PWM_Start(&htim3,TIM_CHANNEL_4);
  HAL_TIM_Encoder_Start(&htim2, TIM_CHANNEL_ALL);
  HAL_TIM_Encoder_Start(&htim4, TIM_CHANNEL_ALL);

  Clear_Usart(&RX1); //初始化串口结构体
  __HAL_UART_ENABLE_IT(&huart1,UART_IT_IDLE);	//使能串口2的空闲中断
  HAL_UART_Receive_DMA(&huart1,(uint8_t*)RX1.rx_buff,RX_BUF_MAX_LEN);	//打开DMA传输模式

OLED_ShowStr(45,2,"C Y H",2);
HAL_Delay(1500);
 	OLED_CLS();
  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
    {
      int a;
      if(RX1.flag == 1)	//判断接收完成
        {
										HAL_GPIO_WritePin(GPIOC,GPIO_PIN_13,0);

          printf("%s\r\n",RX1.rx_buff);		//打印发送的数据
 
          if(strstr((const char *)RX1.rx_buff,"go") != NULL)
            {
              a = 1;//GO
            }
          else if(strstr((const char *)RX1.rx_buff,"back") != NULL)
            {
              a = 2;//BACK
            }
          else if(strstr((const char *)RX1.rx_buff,"left") != NULL)
            {
              a = 3;//LEFT
            }
          else if(strstr((const char *)RX1.rx_buff,"right") != NULL)
            {
              a = 4;//RIGHT
            }
          else if(strstr((const char *)RX1.rx_buff,"stop") != NULL)
            {
              a = 5;//STOP
            }
          Clear_Usart(&RX1);	//清空串口结构体
						HAL_Delay(10);
													HAL_GPIO_WritePin(GPIOC,GPIO_PIN_13,1);

          HAL_UART_Receive_DMA(&huart1,(uint8_t*)RX1.rx_buff,RX_BUF_MAX_LEN);		//打开DMA模式
          HAL_UART_DMAResume(&huart1);		//恢复DMA传输
        }


      switch(a)
        {
        case 1:
          ModePID1();
          ModePID2();
          MOTOR_GO();
          break;
        case 2:
          ModePID1();
          ModePID2();
          MOTOR_BACK();
          break;
        case 3:
          ModePID1();
          ModePID2();
          MOTOR_LEFT();
          break;
        case 4:
          ModePID1();
          ModePID2();
          MOTOR_RIGHT();
          break;
        case 5:
          ModePID1();
          ModePID2();
          MOTOR_STOP();
          break;
        default:
          break;

        }










    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
    }
  /* USER CODE END 3 */
}

